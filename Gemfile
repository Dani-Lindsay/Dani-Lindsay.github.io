#mine+theres
source 'https://rubygems.org'

gem "github-pages", group: :jekyll_plugins
gem "jekyll-include-cache", group: :jekyll_plugins
gem "minimal-mistakes-jekyll"

group :jekyll_plugins do
  gem 'jekyll-paginate'
  gem 'jekyll'
  gem 'jekyll-feed'
  gem 'jekyll-sitemap'
  gem 'jekyll-gist'
  gem 'jemoji'
  gem 'webrick', '~> 1.8'
end

